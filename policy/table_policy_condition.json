{
    "Version": "2012-10-17",
    "Statement": [{
        "Effect": "Allow",
        "Action": ["dynamodb:Scan"],
        "Resources": ["arn:aws:dynamodb:ap-northeast-1:100449931955:table/learnjs"],
        "Condition": {
            "ForAllValues:StringEquals": {
                "dynamodb:Select": ["COUNT"]
            }
        }
    }]
}